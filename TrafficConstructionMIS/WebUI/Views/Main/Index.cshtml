	<script src="~/Assets/Infinity/assets/js/fullcalendar.js"></script>
    <script>
        function clickMenu(url) {
            $("#myFrame").attr("src", url);
        }
        var height = window.innerHeight - 90;
        $("#myFrame").css("height", height);
    </script>
</body>
</html>