	<script src="~/Assets/Infinity/libs/bower/jQuery-Storage-API/jquery.storageapi.min.js"></script>
	<script src="~/Assets/Infinity/libs/bower/bootstrap-sass/assets/javascripts/bootstrap.js"></script>
